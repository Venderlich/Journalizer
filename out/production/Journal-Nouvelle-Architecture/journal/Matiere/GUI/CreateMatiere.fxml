<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="journal.Matiere.GUI.CreateMatiereController">
    <Button layoutX="195.0" layoutY="200.0" mnemonicParsing="false" text="Valider" onAction="#createMatiere">
        <font>
            <Font size="16.0" />
        </font>
    </Button>
    <Label fx:id="lblInfo"  alignment="CENTER" layoutX="195.0" layoutY="98.0" text="Ajouter une matiere" textAlignment="CENTER" wrapText="false" />
    <TextField fx:id="txtMatiere" layoutX="195.0" layoutY="140.0" promptText="Inserer une filiere">
        <font>
            <Font size="16.0" />
        </font>
    </TextField>
</AnchorPane>